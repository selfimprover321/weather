{
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": "error",
      "no-undef": "error",
      "no-unused-vars": ["warn", { "args": "none" }],
      "no-console": "warn",
      "eqeqeq": "error",
      "strict": ["error", "global"]
    }
  }
  